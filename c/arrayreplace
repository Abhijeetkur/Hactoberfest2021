#include<stdio.h>
int main()
{
    int n,p,a[4],i;
    printf("enter the array elements = ");
    for ( int i = 0; i <=3; i++)
    {
      scanf("%d",&a[i]);
    }
    
    printf("enter the element which you want to enter = ");
    scanf("%d",&p);

    printf("enter the position which you want to enter = ");
    scanf("%d",&n);
    if (n<=3);
    {
     for(i=3;i>3;i--)    
     {  a[i+1]=a[i];
     }
    a[n]=p;
    printf("the array is =");
    for ( int i = 0; i <=3; i++)
    printf("%d",a[i]);
    }
    return 0;
}
